    "lint-staged": "^15.2.0",
    "prettier": "^3.6.2",
    "prisma": "^6.11.0",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
