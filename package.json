{
  "name": "cosereeden",
  "version": "1.0.0",
  "description": "CoserEden - 专业Cosplay创作者平台 - Next.js 14 + tRPC + Prisma",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:all": "npm run test && npm run test:integration",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-production": "tsx prisma/seed-production.ts",
    "pm2:start": "./scripts/pm2-manager.sh start production",
    "pm2:stop": "./scripts/pm2-manager.sh stop",
    "pm2:restart": "./scripts/pm2-manager.sh restart production",
    "pm2:status": "./scripts/pm2-manager.sh status",
    "pm2:logs": "./scripts/pm2-manager.sh logs",
    "deploy:production": "./scripts/deploy-production.sh",
    "deploy:rollback": "./scripts/rollback.sh",
    "health-check": "./scripts/health-check.sh",
    "clean": "rm -rf .next node_modules",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
